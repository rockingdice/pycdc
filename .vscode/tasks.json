{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "cmake生成win64项目",
            "type": "shell",
            "command": "cmake",
            "args": [
                "../..",
                "-GVisual Studio 17 2022",
                "-A x64"      
            ],
            "group": "build",
            "options": {
                "cwd": "${workspaceFolder}/build/build-win64"
            }
        },
    ]
}